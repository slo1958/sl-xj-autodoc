	#tag Event
		Sub Pressed()
		  Var dlg As New SelectFolderDialog
		  dlg.ActionButtonCaption = "Select"
		  dlg.Title = "Title Property"
		  dlg.PromptText = "Please select the folder containing the project"
		  dlg.InitialFolder = SpecialFolder.Documents
		  
		  var fld as FolderItem =  dlg.ShowModal
		  if fld = nil then Return
		  
		  TextField1.text = fld.NativePath
		  
		  
		End Sub
