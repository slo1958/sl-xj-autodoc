		Sub Opening()
		  
		  me.RemoveAllRows
		  
		  for i as integer = 0 to 4
		    me.AddRow("")
		    
		  next
		  
		  me.ColumnTypeAt(0) = DesktopListBox.CellTypes.TextField
		  me.CellTextAt(0)="lib-data"
		  
		End Sub
	#tag EndEvent
